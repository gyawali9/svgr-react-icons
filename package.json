{
  "name": "svgr-react-icons",
  "version": "1.0.2",
  "description": "A dynamic SVG icon package for React with size and fill customization",
  "main": "dist/svgr-react-icons.cjs.js",
  "module": "dist/svgr-react-icons.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/svgr-react-icons.es.js",
      "require": "./dist/svgr-react-icons.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "test": "vitest",
    "prepublishOnly": "yarn build"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.1",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/expect": "^2.1.1",
    "jsdom": "^25.0.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^2.1.1"
  },
  "keywords": [
    "react",
    "react-icons",
    "svg-icons",
    "custom-icons",
    "dynamic-icons",
    "react-component",
    "ui-icons",
    "vite-plugin",
    "svgr",
    "typescript",
    "react19",
    "esm",
    "cjs"
  ],
  "author": "Roshan Gyawali",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gyawali9/svgr-react-icons"
  },
  "dependencies": {
    "dompurify": "^3.2.6"
  }
}
